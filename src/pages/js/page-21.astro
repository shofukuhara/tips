---
import Layout from '../../layouts/Layout.astro';
import Header from '../../layouts/Header.astro';
import Button from '../../components/Button.astro';
import ImageFrame from '../../modules/ImageFrame.astro';
const imagePath = './assets/images/';
// headタグ
const metadata = {
  title: 'Tips | js 要素動かす',
  description: 'Tips | js googleカレンダー',
};
---

<Layout {...metadata} pageData="page21">
  <Header />
  <div class="p-page21">
    <div class="p-page_item" data-calendar></div>
  </div>
</Layout>

<style lang="scss">
  @use '../../styles/foundation/setting/mixin' as *;
  @use '../../styles/foundation/setting/function' as *;
  @use '../../styles/foundation/variable/color' as *;
  .p-page21 {
    width: 100%;
    height: 100dvh;
    display: flex;
    align-items: center;
    justify-content: center;
    .p-page_item {
      width: 600px;
      height: auto;
    }
  }
</style>
