---
import Layout from '../../layouts/Layout.astro';
import Header from '../../layouts/Header.astro';
import Button from '../../components/Button.astro';
import ImageFrame from '../../modules/ImageFrame.astro';
const imagePath = './assets/images/';
// headタグ
const title = 'Tips | js スクロールに応じて拡大(GSAP)';
const description = 'Tips | js スクロールに応じて拡大(GSAP)';
---

<Layout {title} {description} pageData="page05">
  <Header />
  <div class="p-page05">
    <div class="p-page05_fv">
      <div class="p-page05_inner">
        <h2 class="p-page_title" data-split>スクロールに応じて拡大</h2>
      </div>
    </div>
    <div class="p-page05_section" data-section>
      <div class="p-page05_inner">
        <div class="p-page05_img" data-image>
          <ImageFrame srcPaths={['bg.jpg']} classNames="i" loading="eager" />
        </div>
      </div>
    </div>
  </div>
</Layout>
